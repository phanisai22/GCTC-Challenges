package com.OOPS.StringBuffer;

public class Palindrome {
	
	public static void main(String[] args) {
		
		StringBuffer text = new StringBuffer("Malayala");
		StringBuffer result = text.reverse();
		if (text.equals(result)) System.out.println(result);//System.out.println("Palindrome");
		else System.out.println("Not a palindrome");
		
	}

}
