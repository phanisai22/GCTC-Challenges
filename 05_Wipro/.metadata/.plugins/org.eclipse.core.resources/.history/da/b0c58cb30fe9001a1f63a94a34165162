package com.Package.automobile.ship;

public abstract class compartment {

    private int height;
    private int width;
    private int breadth;

    public compartment(int height, int width, int breadth){
        this.height = height;
        this.width = width;
        this.breadth = breadth;
    }

    public int getBreadth() {
        return breadth;
    }

    public int getHeight() {
        return height;
    }

    public int getWidth() {
        return width;
    }

}	
